    "class Tudui(nn.Module):\n",
    "    def __init__(self):\n",
    "        super().__init__()\n",
    "\n",
    "    def forward(self, input):\n",
    "        output = input + 1\n",
    "        return output\n",
    "    \n",
    "tudui = Tudui()\n",
    "x = torch.tensor(1.0)\n",
    "output = tudui(x)\n",
    "print(output)"
